<h1>LUsers</h1>

<table>
  <tr>
    <th>First Name</th>
    <th>Middle Name</th>
    <th>Last Name</th>
    <th>Phone</th>
    <th>Email</th>
    <th>Job Description</th>
    <th></th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.first_name %></td>
      <td><%= user.middle_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.phone %></td>
      <td><%= user.email %></td>
      <td><%= user.job_description %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
    </tr>
  <% end %>
</table>